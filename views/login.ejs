<body>

  

<div class="container col-md-6" id="container-login">

    <div class="card">
      <div class="card-header">
        Login
      </div>
        <div class="card-body">

           <!--Not a user alert-->
           <% if(typeof  notUserAlert != 'undefined'){ %>
            <div class="alert alert-danger d-flex align-items-center" role="alert">
              <%= notUserAlert %> 
            </div>
          <% } %> 

          <!--incorrect password alert-->
          <% if(typeof  incorrectPasswordAlert != 'undefined'){ %>
            <div class="alert alert-danger d-flex align-items-center" role="alert">
              <%= incorrectPasswordAlert %> 
            </div>
          <% } %> 


<!--actual form code here-->
          <form action="/login" method="post" id="login-form">
            <div class="form-group row">
                <label for="email" class="col-md-3 col-form-label">UNT E-Mail Address:</label>
                <div class="col-md-7">
                  <!--LEAVE THE INPUTS THE SAMEEEE PLS DONT ADD ALEXIS CODE KEEP THIS -->
                  <input type="text" id="email" name="email" placeholder="Enter your email" required autocomplete="off" class="form-control">
                </div>
              </div>

              <div class="half-line"></div>
              
  
              
                <div class="form-group row">
                  <label for="password" class="col-md-3 col-form-label">Password</label>
                  <div class="col-md-7">
                    <!--LEAVE THE INPUTS THE SAMEEEE PLS DONT ADD ALEXIS CODE KEEP THIS -->
                    <input class="form-control" name="password" type="password" name="password" id="password" placeholder="password" required>
                  </div>
               </div>
          <!--submit button here-->
               <div class="col-md-6 offset-md-4">

                <!--keep the button please-->
                <button type="submit" class="submit-btn">LOGIN</button> 
               </div>
                
              </div>
          </form>
          <div class="account-text"><p>Don't have an account? <a class="link-color" href="/register">Register here</a></p>
        </div>
      </div>
      </div>

     
</body>
</html>
